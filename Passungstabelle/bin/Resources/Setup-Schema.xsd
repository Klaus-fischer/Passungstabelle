                              <xs:attribute name="TabSpalteVorbearbeitungsAbmaße" type="xs:string" />
                              <xs:attribute name="TabSpalteVorbearbeitungsToleranzMitte" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="Formatname" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
